{    
    "name": "RedirectBaidu",  
    "manifest_version":2,  
    "version": "0.7",    
    "description": "redirect baidu homepage to your own webpage",    
    "browser_action": {    
       
       "default_title": "RedirectBaidu"
     },  

     "content_scripts": [{  
         "matches": ["http://www.baidu.com/"],   
         "js": ["redirect.js"],   
         "run_at": "document_start",  
         "all_frames": true   
    }]
    
}  